<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:overScrollMode="never"
    android:scrollbars="none" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/tv_fhr_result_details_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/measure_result_details_time_margin_bottom"
            android:layout_marginLeft="@dimen/measure_result_details_time_margin_left"
            android:layout_marginRight="@dimen/measure_result_details_time_margin_right"
            android:layout_marginTop="@dimen/measure_result_details_time_margin_top"
            android:alpha="@dimen/five_tenth"
            android:drawableLeft="@drawable/detailsoftheresultsview_ic_time"
            android:drawablePadding="@dimen/measure_result_details_time_margin_right"
            android:text="123"
            android:textColor="@android:color/black" />

        <TextView
            android:id="@+id/tv_fhr_heart_rate"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_fhr_result_details_time"
            android:gravity="center"
            android:text="--"
            android:textColor="@color/font_history_high_priority_value"
            android:textSize="@dimen/font_audio_measure_result_vlaue"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tv_unit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_fhr_heart_rate"
            android:alpha="@dimen/two_tenth"
            android:gravity="center"
            android:text="@string/heart_rate_unit"
            android:textColor="@android:color/black"
            android:textSize="@dimen/font_audio_measure_result_util_C" />

        <LinearLayout
            android:id="@+id/ll_fetal_heart_avg"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_unit"
            android:layout_marginBottom="20dp"
            android:layout_marginTop="5dp"
            android:gravity="center_horizontal"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:alpha="@dimen/four_tenth"
                android:text="@string/average_fhr"
                android:textColor="@android:color/black"
                android:textSize="@dimen/font_fhr_result_avg_heart" />

            <TextView
                android:id="@+id/tv_fhr_avg_heart_rate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:alpha="@dimen/four_tenth"
                android:text="0"
                android:textColor="@android:color/black"
                android:textSize="@dimen/font_fhr_result_avg_heart" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:alpha="@dimen/two_tenth"
                android:text="@string/heart_rate_unit"
                android:textColor="@android:color/black"
                android:textSize="@dimen/font_fhr_result_avg_heart" />
        </LinearLayout>

        <View
            android:id="@+id/view_div_top"
            android:layout_width="match_parent"
            android:layout_height="@dimen/divider_height"
            android:layout_below="@+id/ll_fetal_heart_avg"
            android:background="@color/divider_bottom" />

        <LinearLayout
            android:id="@+id/ll_chart"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/sample_height"
            android:layout_below="@+id/view_div_top"
            android:orientation="vertical" >

            <FrameLayout
                android:id="@+id/dynamic_polyline"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/sample_height" />
        </LinearLayout>

        <View
            android:id="@+id/view_div_bottom"
            android:layout_width="match_parent"
            android:layout_height="@dimen/divider_height"
            android:layout_below="@+id/ll_chart" />

        <RelativeLayout
            android:id="@+id/playback_progress_bar"
            android:layout_width="fill_parent"
            android:layout_height="53dp"
            android:layout_below="@+id/view_div_bottom"
            android:background="#F7FCFB" >

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divider_height"
                android:background="@color/divider_bottom" />

            <ImageView
                android:id="@+id/playback_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_margin="2dp"
                android:background="@drawable/btn_fhr_start" />

            <TextView
                android:id="@+id/playback_current_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/playback_btn"
                android:paddingLeft="5dp"
                android:text="00:00" />

            <SeekBar
                android:id="@+id/playback_seek"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/playback_total_time"
                android:layout_toRightOf="@+id/playback_current_time"
                android:max="0"
                android:progress="0"
                android:progressDrawable="@drawable/seekbar_img"
                android:thumb="@drawable/seekbar_thumb"
                android:thumbOffset="5dp" />

            <TextView
                android:id="@+id/playback_total_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:paddingRight="5dp"
                android:text="00:00" />
        </RelativeLayout>

        <View
            android:id="@+id/view_seekbar_bottom"
            android:layout_width="match_parent"
            android:layout_height="@dimen/divider_height"
            android:layout_below="@+id/playback_progress_bar" />

        <TextView
            android:id="@+id/tv_fhr_result_details_remark"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/view_seekbar_bottom"
            android:layout_marginLeft="@dimen/measure_result_value_margin"
            android:layout_marginRight="@dimen/measure_result_value_margin"
            android:layout_marginTop="10dp"
            android:background="@drawable/item_readme_bg"
            android:hint="添加备注"
            android:padding="@dimen/oxygen_result_details_readme_padding"
            android:textSize="@dimen/font_pressure_measure_change_unit" />

        <RelativeLayout
            android:id="@+id/layout_more_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_fhr_result_details_remark"
            android:layout_marginBottom="@dimen/measure_result_value_margin"
            android:layout_marginLeft="@dimen/measure_result_value_margin"
            android:layout_marginRight="@dimen/measure_result_value_margin"
            android:layout_marginTop="20dp" >

            <include layout="@layout/item_more_data" />
        </RelativeLayout>

        <View
            android:id="@+id/view_div_remark"
            android:layout_width="match_parent"
            android:layout_height="@dimen/divider_height"
            android:layout_below="@+id/layout_more_data"
            android:background="@color/divider_bottom" />

        <LinearLayout
            android:id="@+id/ll_reference_example"
            android:layout_width="match_parent"
            android:layout_height="@dimen/measure_result_details_history_trend_high"
            android:layout_below="@+id/view_div_remark"
            android:layout_marginLeft="@dimen/measure_result_value_margin"
            android:layout_marginRight="@dimen/measure_result_value_margin"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/reference_example"
                android:textColor="#7185AA"
                android:textSize="@dimen/font_pressure_history_list_big" />

            <View
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1" />

            <ImageView
                android:id="@+id/fetal_open"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/ic_pickup" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/fetal_reference"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ll_reference_example"
            android:background="#FAFBFC"
            android:orientation="vertical"
            android:visibility="gone" >

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divider_height"
                android:background="@color/divider_bottom" />

            <LinearLayout
                android:id="@+id/fl_simple_chart"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/sample_height"
                android:background="@android:color/white"
                android:gravity="center_horizontal"
                android:orientation="vertical" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="-20dp"
                android:background="@android:color/white"
                android:paddingLeft="@dimen/sample_margin_horizal"
                android:paddingRight="@dimen/sample_margin_horizal"
                android:text="@string/fetalheat_sample_tip"
                android:textColor="@color/gray" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divider_height"
                android:background="@color/divider_bottom" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/measure_result_details_history_trend_high"
                android:layout_marginLeft="@dimen/measure_result_value_margin"
                android:layout_marginRight="@dimen/measure_result_value_margin"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/fetal_left"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_left" />

                <TextView
                    android:id="@+id/fetal_text"
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/example_diagram_of_fetal_hypoxia"
                    android:textColor="#ff565657"
                    android:textSize="@dimen/font_connection_start_text" />

                <ImageView
                    android:id="@+id/fetal_right"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_right" />
            </LinearLayout>
        </LinearLayout>
        <!-- </LinearLayout> -->

        <View
            android:id="@+id/view_divider"
            android:layout_width="match_parent"
            android:layout_height="@dimen/divider_height"
            android:layout_below="@+id/fetal_reference"
            android:background="@color/divider_bottom"
            android:visibility="visible" />
        <!-- 由此替换 -->

        <RelativeLayout
            android:id="@+id/item_nearly_measurement"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/view_divider"
            android:layout_marginLeft="@dimen/measure_result_value_margin"
            android:layout_marginRight="@dimen/measure_result_value_margin" >

            <include layout="@layout/item_nearly_measurement" />
        </RelativeLayout>
        <!-- 到此结束 -->

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/item_nearly_measurement"
            android:orientation="vertical"
            android:paddingBottom="@dimen/suggest_margin" >

            <com.medzone.cloud.comp.widget.CloudWebView
                android:id="@+id/result_details_suggestWV"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >
            </com.medzone.cloud.comp.widget.CloudWebView>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divider_height"
                android:background="@color/divider_bottom"
                android:visibility="gone" />

            <TextView
                android:id="@+id/bs_result_details_suggestTV"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:alpha="@dimen/five_tenth"
                android:gravity="center_horizontal"
                android:paddingTop="@dimen/suggest_margin"
                android:text="@string/result_details_suggest_tv"
                android:textColor="@android:color/black"
                android:textSize="@dimen/font_measure_result_details_history_trend_text"
                android:visibility="gone" />
        </LinearLayout>
    </RelativeLayout>

</ScrollView>